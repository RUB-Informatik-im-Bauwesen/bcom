<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TimeOfSampling>
  a owl:DatatypeProperty ;
  rdfs:domain <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#FreshConcretePropertySet> ;
  rdfs:label "Time of sampling"@en ;
  rdfs:comment "Time of sampling with format year-month-day in yyyy-mm-ddThh-mm-ss"@en ;
  rdfs:range xsd:dateTime .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TypeOfTest>
  a owl:DatatypeProperty ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConcreteWork>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConformityOfCompressiveStrength>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#FreshConcretePropertySet>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSample>
   )
  ] ;
  rdfs:label "Type of test"@en ;
  rdfs:comment "Type of test as self-inspection or third-party monitoring"@en ;
  rdfs:range [
    a rdfs:Datatype ;
    owl:oneOf (
     "SelfInspection"
     "Third-Party Monitoring"
   )
  ] .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ValueOfBreakingLoad>
