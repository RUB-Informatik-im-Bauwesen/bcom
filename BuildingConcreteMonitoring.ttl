<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSampleAssignedTo>
  a owl:ObjectProperty, owl:Class ;
  owl:inverseOf <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#HasTestSample> ;
  rdfs:subClassOf owl:TopObjectProperty, owl:Thing ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConcreteWork>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConformityOfCompressiveStrength>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#FreshConcretePropertySet>
   )
  ] ;
  rdfs:domain <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSample> ;
  rdfs:label "Test Sample assigned to"@en, "Test Sample Assigned To Class"@en .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#Height>
  a owl:DatatypeProperty ;
  rdfs:domain <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSample> ;
  rdfs:label "Height of sample"@en ;
  rdfs:comment "Height of a test sample as cube in mm"@en ;
  rdfs:range xsd:decimal .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#Length>
  a owl:DatatypeProperty ;
  rdfs:domain <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSample> ;
  rdfs:label "Length of sample"@en ;
  rdfs:comment "Length of a test sample for cube in mm"@en ;
  rdfs:range xsd:decimal .

