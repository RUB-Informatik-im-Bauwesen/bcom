  owl:inverseOf <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestReportAssignedTo> ;
  rdfs:domain <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConformityOfCompressiveStrength> ;
  rdfs:label "Test report"@en ;
  rdfs:comment "Report of conformity test of compressive strength"@en ;
  rdfs:range xsd:anyURI .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestReportAssignedTo>
  a owl:ObjectProperty, owl:Class ;
  owl:inverseOf <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#HasTestReport> ;
  rdfs:subClassOf owl:TopObjectProperty, owl:Thing ;
  rdfs:domain xsd:anyURI ;
  rdfs:range <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConformityOfCompressiveStrength> ;
  rdfs:label "Test Report assigned to"@en .

<http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#HasTestSample>
  a owl:ObjectProperty ;
  owl:inverseOf <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSampleAssignedTo> ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConcreteWork>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConformityOfCompressiveStrength>
     <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#FreshConcretePropertySet>
   )
  ] ;
  rdfs:range <http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#TestSample> ;
