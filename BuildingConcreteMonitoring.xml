  <owl:DatatypeProperty rdf:about="http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ClassOfMonitoring">
    <rdfs:domain rdf:resource="http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring#ConcreteWork"/>
    <rdfs:label xml:lang="en">Class Of concrete monitoring</rdfs:label>
    <rdfs:comment xml:lang="en">Class Of concrete monitoring according to DIN EN 13670/DIN 1045-3 related to the properties of hardened concrete</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:Description>
            <rdf:first>Class 1</rdf:first>
            <rdf:rest>
              <rdf:Description>
                <rdf:first>Class 2</rdf:first>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first>Class 3</rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:oneOf>

      </rdfs:Datatype>
    </rdfs:range>

  </owl:DatatypeProperty>

