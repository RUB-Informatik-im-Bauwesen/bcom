<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:bcom="http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring">
  <owl:Ontology rdf:about="">
    <owl:versionIRI xml:lang="en">http://www.inf.bi.rub.de/semweb/ns/buildingconcretemonitoring</owl:versionIRI>
    <dc:source>https://github.com/RUB-Informatik-im-Bauwesen/bcom</dc:source>
    <dc:license>https://creativecommons.org/licenses/by/4.0/</dc:license>
    <dc:description xml:lang="en">The Building Concrete Monitoring Ontology (BCOM) is defined for capturing information of concrete work, concrete curing and testing of concrete properties.</dc:description>
    <dc:title xml:lang="en">Building Concrete Monitoring Ontology (BCOM)</dc:title>
    <owl:versionInfo xml:lang="en">0.2</owl:versionInfo>
    <dc:creator>
      <foaf:Person>
        <foaf:name>Philipp Hagedorn, Ruhr-University Bochum</foaf:name>
      </foaf:Person>
    </dc:creator>
    <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2021-01-20</dc:created>
    <dc:creator>
      <foaf:Person>
        <foaf:name>Liu Liu, Ruhr-University Bochum</foaf:name>
      </foaf:Person>
    </dc:creator>
  </owl:Ontology>
  <owl:Class rdf:ID="ConformityOfCompressiveStrength">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Test"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Conformity testing of compressive strength</rdfs:label>
    <rdfs:comment xml:lang="en">This class provides the necessary information to perform and evaluate the conformity testing of concrete compressive strength.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Test">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Test</rdfs:label>
    <rdfs:comment xml:lang="en">Represents the super-class for different types of tests for placed concrete</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ConcreteDelivery">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Concrete delivery</rdfs:label>
    <rdfs:comment xml:lang="en">This class provides information about the delivery of concrete</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TestSample">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Test sample</rdfs:label>
    <rdfs:comment xml:lang="en">This class provides the information of test sample for conformity test of concrete compressive strength</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Storage">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Storage of a test sample</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents the storage conditions of a test sample</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ConcreteCuring">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Concrete curing</rdfs:label>
    <rdfs:comment xml:lang="en">This class provides the necessary information of concrete curing after concreting</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FreshConcreteProperties">
    <rdfs:subClassOf rdf:resource="#Test"/>
    <rdfs:label xml:lang="en">Fresh concrete property testing</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents a group of fresh concrete property.  A fresh concrete property set object will usually contain density, voids ratio, consistency.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ConcretePlacement">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">Concrete placement</rdfs:label>
    <rdfs:comment xml:lang="en">This class provides the necessary information of current concreting of the construction</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="HasDelivery">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="SuppliesConcreteTo"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#ConcreteDelivery"/>
    <rdfs:label xml:lang="en">has Concrete Supplier</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasStorage">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="StoresSample"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Storage"/>
    <rdfs:domain rdf:resource="#TestSample"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasTestSample">
    <rdfs:range rdf:resource="#TestSample"/>
    <rdfs:label xml:lang="en">has Test Sample</rdfs:label>
    <rdfs:domain rdf:resource="#Test"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasConcretePlacement">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="HasCuring"/>
    </owl:inverseOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#TopObjectProperty"/>
    <rdfs:range rdf:resource="#ConcretePlacement"/>
    <rdfs:label xml:lang="en">Curing has Concrete Work</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteCuring"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasTestPerson">
    <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#Individual"/>
    <rdfs:label xml:lang="en">has Perform Test Person</rdfs:label>
    <rdfs:domain rdf:resource="#Test"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasTestOrganisation">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="HasTestSubjects"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#Organization"/>
    <rdfs:label xml:lang="en">has Perform Test Organisation</rdfs:label>
    <rdfs:domain rdf:resource="#Test"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasTest">
    <rdfs:range rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:label xml:lang="en">has Test Of Compressive Strength</rdfs:label>
    <rdfs:domain rdf:resource="#Test"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#SuppliesConcreteTo">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="HasSupplier"/>
    </owl:inverseOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#TopObjectProperty"/>
    <rdfs:range rdf:resource="#ConcretePlacement"/>
    <rdfs:label xml:lang="en">supplies Concrete to</rdfs:label>
    <rdfs:label xml:lang="en">Supplies Concrete To</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/vcard/ns#Organization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#HasSupplier">
    <owl:inverseOf rdf:resource="#SuppliesConcreteTo"/>
    <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#Organization"/>
    <rdfs:label xml:lang="en">has Concrete Supplier</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteDelivery"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="DeliversConcreteFor">
    <owl:inverseOf rdf:resource="#HasDelivery"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#TopObjectProperty"/>
    <rdfs:range rdf:resource="#ConcretePlacement"/>
    <rdfs:label xml:lang="en">Delivers concrete for</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteDelivery"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#HasCuring">
    <owl:inverseOf rdf:resource="#HasConcretePlacement"/>
    <rdfs:range rdf:resource="#ConcreteCuring"/>
    <rdfs:label xml:lang="en">has Curing</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#StoresSample">
    <owl:inverseOf rdf:resource="#HasStorage"/>
    <rdfs:range rdf:resource="#TestSample"/>
    <rdfs:domain rdf:resource="#Storage"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#HasTestSubjects">
    <owl:inverseOf rdf:resource="#HasTestOrganisation"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#TopObjectProperty"/>
    <rdfs:range rdf:resource="#Test"/>
    <rdfs:label xml:lang="en">has Test Subjects</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/vcard/ns#Organization"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="TestCharacteristic">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Tested property</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">The property has to be tested</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Weigth">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Weigth of sample</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Weigth of a test sample in kg</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="AirTemperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Air temperature</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ConcretePlacement"/>
          <owl:Class rdf:about="#TestSample"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">Air temperature during concreting or test on fresh concrete in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ClassOfMonitoring">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first>Class 3</rdf:first>
            </rdf:rest>
            <rdf:first>Class 2</rdf:first>
          </rdf:rest>
          <rdf:first>Class 1</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en">Class Of concrete monitoring</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
    <rdfs:comment xml:lang="en">Class Of concrete monitoring according to DIN EN 13670/DIN 1045-3 related to the properties of hardened concrete</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="RateOfFlowTableTest">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Rate of flow table test</rdfs:label>
    <rdfs:domain rdf:resource="#FreshConcreteProperties"/>
    <rdfs:comment xml:lang="en">Rate of flow table test according to DIN EN 12350-5 in mm</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Condition">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Storage condition</rdfs:label>
    <rdfs:domain rdf:resource="#Storage"/>
    <rdfs:comment xml:lang="en">Description of storage condition</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="DeliveryNote">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label xml:lang="en">Delivery note</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteDelivery"/>
    <rdfs:comment xml:lang="en">Delivery note of mixed-ready concrete</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="WidthOrDia">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Width or diameter of sample</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Width of a test sample as cube or diameter of a test sample as cylinder in mm</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="FinalCompressiveStrength">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Result of compressive strength</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Result of the compressive strength of the respective test sample  with consideration of the conversion factor in N/mm2</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="SamplingDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">Date of sampling</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Date of sampling with format year-month-day in yyyy-mm-dd</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="TestDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">Date of testing</rdfs:label>
    <rdfs:domain rdf:resource="#Test"/>
    <rdfs:comment xml:lang="en">Date of testing with format year-month-day in yyyy-mm-dd</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Length">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Length of sample</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Length of a test sample for cube in mm</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="MaturityOfConcrete">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Maturity of concrete</rdfs:label>
    <rdfs:domain rdf:resource="#FreshConcreteProperties"/>
    <rdfs:comment xml:lang="en">Maturity of concrete in minute</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Height">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Height of sample</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Height of a test sample as cube in mm</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="FreshConcreteTemperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Fresh concrete temperature before concreting</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
    <rdfs:comment xml:lang="en">Fresh concrete temperature before concreting in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Temperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Storage temperature</rdfs:label>
    <rdfs:domain rdf:resource="#Storage"/>
    <rdfs:comment xml:lang="en">Storage temperature in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Humidity">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Storage humidity</rdfs:label>
    <rdfs:domain rdf:resource="#Storage"/>
    <rdfs:comment xml:lang="en">Humidity of storage condition in %</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ObjectTemperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Temperature of sample</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Temperature of a test sample in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="MinAirTemperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Min temperature</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteCuring"/>
    <rdfs:comment xml:lang="en">Minimum temperature during post-treatment in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="TestedDensity">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Density of sample</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Density of a sample in kg/m3</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ConversionFactor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Conversion factor</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Conversion factor of compressive strength testing</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="BeginDate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">Begin date of concrete curing</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteCuring"/>
    <rdfs:comment xml:lang="en">Begin date of concrete curing with format year-month-day in yyyy-mm-dd</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="DateOfStripping">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">Date of stripping</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
    <rdfs:comment xml:lang="en">Date of stripping with format year-month-day in yyyy-mm-dd</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="BulkDensity">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Bulk density</rdfs:label>
    <rdfs:domain rdf:resource="#FreshConcreteProperties"/>
    <rdfs:comment xml:lang="en">Bulk density of fresh concrete in kg/m3</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="VoidsRatio">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Voids ratio</rdfs:label>
    <rdfs:domain rdf:resource="#FreshConcreteProperties"/>
    <rdfs:comment xml:lang="en">Voids ratio of fresh concrete in V-%</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="DateOfConcreting">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">Date of concreting</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
    <rdfs:comment xml:lang="en">Date of concreting with format year-month-day in yyyy-mm-dd</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="InterimCompressiveStrength">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Intermediate result of compressive strength</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Intermediate result of the compressive strength of the respective sample without conversion factor of in N/mm2</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ConcreteTemperature">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Fresh concrete temperature</rdfs:label>
    <rdfs:domain rdf:resource="#FreshConcreteProperties"/>
    <rdfs:comment xml:lang="en">Fresh concrete temperature during testing of property in Celsius</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="QuantityOfConcrete">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Quantity of concrete</rdfs:label>
    <rdfs:domain rdf:resource="#ConcretePlacement"/>
    <rdfs:comment xml:lang="en">Quantity of the concrete placement in cubature m3</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="BeginTimeOfPlacement">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdfs:label xml:lang="en">Begin time of paving</rdfs:label>
    <rdfs:domain rdf:resource="#ConcreteDelivery"/>
    <rdfs:comment xml:lang="en">Begin time of paving with format hour-minute-second in hh-mm-ss</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="SampleID">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Sample ID</rdfs:label>
    <rdfs:domain rdf:resource="#TestSample"/>
    <rdfs:comment xml:lang="en">Name or ID of the test sample</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="TestType">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first>Third-Party Monitoring</rdf:first>
          </rdf:rest>
          <rdf:first>Self-Inspection</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en">Type of test</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ConcretePlacement"/>
          <owl:Class rdf:about="#Test"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">Type of test as self-inspection or third-party monitoring</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="EvaluationOfTest">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Evaluation of the mean value</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Evaluation of the mean value of the test in N/mm2</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="NormativeReference">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">Test standard</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
    <rdfs:comment xml:lang="en">Norm or standard of the test method</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="TestedBreakingLoad">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label xml:lang="en">Value of breaking load</rdfs:label>
    <rdfs:domain rdf:resource="#ConformityOfCompressiveStrength"/>
